{
  "name": "a2alite",
  "version": "0.0.1",
  "description": "A Modular, extensible SDK (Server & Client) for Agent to Agent (A2A) protocol",
  "keywords": [
    "A2A",
    "AI",
    "Agents",
    "AI",
    "LLM",
    "Agent to Agent"
  ],
  "license": "Apache-2.0",
  "author": "Hamid Alipour",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "pnpm -r build",
    "test": "pnpm -r test",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "clean": "pnpm -r clean",
    "docs:build": "typedoc",
    "docs:serve": "typedoc --serve",
    "publish:sdk": "pnpm --filter @a2alite/sdk publish",
    "version:patch": "pnpm --filter @a2alite/sdk version patch",
    "version:minor": "pnpm --filter @a2alite/sdk version minor"
  },
  "devDependencies": {
    "prettier": "^3.6.2",
    "rimraf": "^5.0.5",
    "typedoc": "^0.28.7",
    "typedoc-plugin-zod": "^1.4.2",
    "typescript": "^5.8.3"
  }
}
